public class GCD{

    /**
     * 欧几里得算法，辗转相除法求两数的最大公约数
     * @param p
     * @param q
     * @return
     */
    public static int gcd(int p, int q)
    {
        if (q == 0){
            return  p;
        }
        int r = p % q;
        return gcd(q, r);
    }
    public static void main(String[] args) {
        int x = gcd(10, 2);
        System.out.printf(x);
    }
}